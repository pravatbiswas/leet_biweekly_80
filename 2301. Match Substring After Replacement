class Solution:
    def successfulPairs(self, spells: List[int], potions: List[int], success: int) -> List[int]:
        s = sorted((x, i) for i, x in enumerate(spells))
        p = sorted(potions)
        n = len(p)
        ret = [None] * len(s)
        i = n
        for x, j in s:
          while i > 0 and x * p[i - 1] >= success:
            i -= 1
          ret[j] = n - i
        return ret
                    
        
